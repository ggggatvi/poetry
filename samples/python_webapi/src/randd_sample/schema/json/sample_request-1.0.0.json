{
    "definitions": {},
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type": "object",
    "title": "Boston house prices dataset schema",
    "required": [
        "CRIM",
        "ZN",
        "INDUS",
        "CHAS",
        "NOX",
        "RM",
        "AGE",
        "DIS",
        "RAD",
        "TAX",
        "PTRATIO",
        "B",
        "LSTAT"
    ],
    "properties": {
        "CRIME": {
            "$id": "#/properties/CRIME",
            "type": "number",
            "title": "CRIM per capita crime rate by town",
            "default": 0,
            "examples": [
                0.32
            ]
        },
        "ZN": {
            "$id": "#/properties/ZN",
            "type": "number",
            "title": "ZN proportion of residential land zoned for lots over 25,000 sq.ft.",
            "default": 0,
            "examples": [
                0.32
            ]
        },
        "INDUS": {
            "$id": "#/properties/INDUS",
            "type": "number",
            "title": "INDUS proportion of non-retail business acres per town",
            "default": 0,
            "examples": [
                0.32
            ]
        },
        "CHAS": {
            "$id": "#/properties/CHAS",
            "type": "number",
            "title": "CHAS Charles River dummy variable (= 1 if tract bounds river; 0 otherwise)",
            "default": 0,
            "examples": [
                0.32
            ]
        },
        "NOX": {
            "$id": "#/properties/NOX",
            "type": "number",
            "title": "NOX nitric oxides concentration (parts per 10 million)",
            "default": 0,
            "examples": [
                0.32
            ]
        },
        "RM": {
            "$id": "#/properties/RM",
            "type": "number",
            "title": "RM average number of rooms per dwelling",
            "default": 0,
            "examples": [
                0.32
            ]
        },
        "AGE": {
            "$id": "#/properties/AGE",
            "type": "number",
            "title": "AGE proportion of owner-occupied units built prior to 1940",
            "default": 0,
            "examples": [
                0.32
            ]
        },
        "DIS": {
            "$id": "#/properties/DIS",
            "type": "number",
            "title": "DIS weighted distances to five Boston employment centres",
            "default": 0,
            "examples": [
                0.32
            ]
        },
        "RAD": {
            "$id": "#/properties/RAD",
            "type": "number",
            "title": "RAD index of accessibility to radial highways",
            "default": 0,
            "examples": [
                0.32
            ]
        },
        "TAX": {
            "$id": "#/properties/TAX",
            "type": "number",
            "title": "TAX full-value property-tax rate per $10,000",
            "default": 0,
            "examples": [
                0.32
            ]
        },
        "PTRATIO": {
            "$id": "#/properties/PTRATIO",
            "type": "number",
            "title": "PTRATIO pupil-teacher ratio by town",
            "default": 0,
            "examples": [
                0.32
            ]
        },
        "B": {
            "$id": "#/properties/B",
            "type": "number",
            "title": "B 1000(Bk - 0.63)^2 where Bk is the proportion of blacks by town",
            "default": 0,
            "examples": [
                0.32
            ]
        },
        "LSTAT": {
            "$id": "#/properties/LSTAT",
            "type": "number",
            "title": "LSTAT % lower status of the population",
            "default": 0,
            "examples": [
                0.32
            ]
        }
    }
}
